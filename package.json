{
  "name": "shopp",
  "version": "1.0.0",
  "description": "ShopP Project",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "mocha -r ts-node/register test/**/*.ts",
    "init-database": "typeorm schema:sync -d ./dist/data.js",
    "format-code": "prettier --write .",
    "generate-migration": "typeorm-ts-node-esm migration:generate ./src/migrations/seed -d ./src/data.ts",
    "run-migration": "typeorm-ts-node-esm migration:run -d ./src/data.ts",
    "revert-migration": "typeorm-ts-node-esm migration:revert -d ./src/data.ts",
    "dev": "nodemon"
  },
  "author": "Le Thanh Long",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "chalk": "^4.1.2",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "helmet": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "node-geocoder": "^4.2.0",
    "nodemailer": "^6.7.8",
    "socket.io": "^4.5.4",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.5.0",
    "typeorm": "^0.3.7"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.2",
    "@types/chai": "^4.3.3",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mocha": "^9.1.1",
    "@types/multer": "^1.4.7",
    "@types/mysql": "^2.15.21",
    "@types/node-geocoder": "^3.24.5",
    "@types/nodemailer": "^6.4.6",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^3.0.1",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
